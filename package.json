{
  "name": "electron-starter-template",
  "version": "0.1.0",
  "description": "Electron Starter Template",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "build": "node scripts/build.js && electron-builder -c config/electron-builder.json",
    "build:win": "node scripts/build.js && electron-builder -c config/electron-builder.json --win",
    "build:mac": "node scripts/build.js && electron-builder -c config/electron-builder.json --mac",
    "build:linux": "node scripts/build.js && electron-builder -c config/electron-builder.json --linux"
  },
  "author": {
    "name": "tsukiseele",
    "url": "https://github.com/tsukiseele"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "~2.2.4",
    "@vue/cli": "~5.0.1",
    "chalk": "~4.1.2",
    "chokidar": "~3.5.3",
    "electron": "~17.4.11",
    "electron-builder": "~23.0.3",
    "pug": "~3.0.2",
    "sass": "~1.49.9",
    "typescript": "~4.6.2",
    "vite": "~2.9.9"
  },
  "dependencies": {
    "@types/crypto-js": "~4.1.1",
    "@types/node": "~17.0.41",
    "esm": "~3.2.25",
    "js-base64": "~3.7.2",
    "lodash": "~4.17.21",
    "p-queue": "~6.6.2",
    "pinia": "~2.0.11",
    "vue": "~3.2.36",
    "vue-router": "~4.0.12"
  }
}
